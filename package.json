{
  "name": "ai-life-assistant",
  "version": "1.0.0",
  "description": "AI-powered life management and goal achievement platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-backend": "tsc",
    "dev-backend": "ts-node src/backend/index.ts",
    "dev-python": "cd python-agents && python app.py",
    "setup-python": "cd python-agents && python -m venv .venv",
    "install-python-deps": "cd python-agents && pip install -r requirements.txt",
    "dev-full": "concurrently \"npm run dev-python\" \"npm start\""
  },
  "keywords": [
    "ai",
    "life-assistant",
    "goals",
    "planning"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^7.6.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^9.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
